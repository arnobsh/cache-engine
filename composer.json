{
  "name": "arnobsh/cache",
  "description": "Cache Engine",
  "license": "",
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": "^5.3|^7.0",
    "psr/cache": "~1.0",
    "psr/simple-cache": "~1.0",
    "bcosca/fatfree": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^7",
    "mockery/mockery": "dev-master",
    "phpunit/php-code-coverage": "^6.0.1",
    "phpmd/phpmd": "^2.6"
  },
  "keywords": [
    "cache", "caching",
    "key", "value", "store",
    "apc",
    "memcached",
    "redis",
    "Cache Engine",
    "Storage Engine",
    "Storage"
  ],
  "authors": [
    {
      "name": "Arnob Saha",
      "email": "arnobsh@gmail.com",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Cache\\Storage\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cache\\Storage\\Tests\\": "tests/"
    }
  }
}
